@model List<EntranceViewModel>
@{
    ViewData["Title"] = "Belépések";
}

@section Styles {
    <link rel="stylesheet" href="~/css/abonaments_clients.css" />
}

<div class="page-header">
    <h2>Belepesek</h2>
</div>
<div class="col-lg-12 row">
    <table class="table">
        <thead>
            <tr>
                
                <th scope="col">Felhasznalo</th>
                <th scope="col">Abonament</th>
                <th scope="col">Datum</th>
            </tr>
        </thead>
        @foreach (var entrance in Model)
        {
            
            <tr>
                
                <td>@entrance.User.FirstName @entrance.User.LastName</td>
                <td>@entrance.Abonament.Name</td>
                <td>@entrance.EnteringTime</td>
            </tr>
           
        }
    </table>
</div>